+ 	pScreenPriv = GetScreenPrivate (pScreen);
+ 	(*pScreenPriv->screenFuncs->NewEventScreen) (pDev, pScreen, FALSE);
+ 	NewCurrentScreen (pDev, pScreen, x, y);
+ 
++        pPointer = MIPOINTER(pDev);
++        if (pPointer == NULL) {
